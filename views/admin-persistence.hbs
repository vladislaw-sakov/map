<body>
<form id="write-testing" action="/persistence/post" method="post">
    <label for="table">Table:</label>
    <input id="table" type="text" name="table"></input>
    <br/>
    <label for="key">Key:</label>
    <input id="key" type="text" name="key"></input>
    <br/>
    <label for="type">Type:</label>
    <select id="type" name="type">
        <option value="S">S</option>
        <option value="N">N</option>
    </select>
    <br></br>
    <div id="valueField">
        <label for="value">Value:</label>
        <input id="value" type="text" name="value"></input>
    </div>
    <br></br>
    <label for="delete">Delete:</label>
    <input id="delete" type="checkbox" name="delete" value="delete" onclick="displayField();">
    <br></br>
    <input type="submit" value="submit"></input>
</form>
</body>
</html>